# IntellectSafe Environment Configuration
# Copy this to .env and fill in your values

# Database
DATABASE_URL=postgresql://user:password@localhost:5432/intellectsafe

# Security (REQUIRED in production)
SECRET_KEY=your-secret-key-here-generate-with-openssl-rand-hex-32
ENVIRONMENT=development

# LLM Provider Keys (at least one required for Council)
GEMINI_API_KEY=
GROK_API_KEY=
GEMINI2_API_KEY=
GROK2_API_KEY=
OPEN_ROUTER_KEY=

# Optional
REDIS_URL=redis://localhost:6379
DEBUG=false
CORS_ORIGINS=http://localhost:5173,https://intellect-safe.vercel.app
